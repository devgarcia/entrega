<!-- Runs different partial form depending on who is editing the trnsporter status -->
<% if @user.class.name == "Company"%>
  <%= form_with(model: transporter, url: [@user, transporter], local: true) do |form| %>
    <%= render 'transporters/company_transporter_form.html.erb', form: form, transporter: @transporter %>
  <% end %>
<% elsif  @user.class.name == "Transporter" %>
  <%= form_with(model: transporter, local: true) do |form| %>
    <%= render 'transporters/transporter_form.html.erb', form: form, transporter: @transporter %>
  <% end %>
<% end %>
